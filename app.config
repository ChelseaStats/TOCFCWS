<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
    </startup>
  <appSettings>
    <add key="url" value="*" />
    <add key="salt" value="*" />
    <add key="containerReference" value="*" />
    <add key="truncateAmount" value="*" />
    <add key="SharedAccessSignatureExpiryTimeOffset" value="*" />

    <!-- storage account-->
    <add key="accountName" value="*" />
    <add key="accountKey" value="*" />

    <!-- twitter -->
    <add key="consumerKey" value="*" />
    <add key="consumerKeySecret" value="*" />
    <add key="accessToken" value="*" />
    <add key="accessTokenSecret" value="*" />

  </appSettings>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Microsoft.Azure.KeyVault.Core" publicKeyToken="31bf3856ad364e35" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-2.0.0.0" newVersion="2.0.0.0" />
      </dependentAssembly>
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-11.0.0.0" newVersion="11.0.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>
